{
  "name": "micro",
  "version": "10.0.0",
  "description": "Asynchronous HTTP microservices",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc -p .",
    "test": "NODE_ENV=test jest",
    "lint": "tslint --project .",
    "prepublishOnly": "yarn && yarn run lint && yarn run test && yarn run build"
  },
  "bin": {
    "micro": "./lib/cli.js"
  },
  "repository": "zeit/micro",
  "keywords": [
    "micro",
    "service",
    "microservice",
    "serverless",
    "API"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/content-type": "^1.1.2",
    "@types/is-stream": "^1.1.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/request-promise": "^4.1.42",
    "@types/test-listen": "^1.1.0",
    "jest": "^23.6.0",
    "request": "2.88.0",
    "request-promise": "4.2.2",
    "resumer": "0.0.0",
    "test-listen": "^1.1.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "arg": "^4.1.0",
    "chalk": "^2.4.2",
    "content-type": "1.0.4",
    "is-stream": "1.1.0",
    "raw-body": "^2.3.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts",
      "!<rootDir>/src/**/*.d.ts"
    ],
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "__tests__/.*_[^/]+\\.ts"
    ],
    "testEnvironment": "node"
  }
}
